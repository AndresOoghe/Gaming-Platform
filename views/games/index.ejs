<h1 class="text-4xl text-primary lg:mb-12 mb-4">Search Games</h1>

<form action="/games" method="get">
    <div>
        <label for="name">Name</label>
    </div>
    <input type="submit" value="Search">
</form>
<% if (typeof games != 'undefined') { %>

<% games.forEach(game => { %>

    <div><%= game.name %></div>

<% }); %>

<% } %>